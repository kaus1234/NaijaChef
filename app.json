{
  "expo": {
    "name": "SabiCook",
    "slug": "sabicook",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.sabicook.app"
    },
    "android": {
      "package": "com.sabicook.app"
    },
    "plugins": [
      "expo-router",
      [
        "expo-font",
        {
          "fonts": ["./assets/fonts"]
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}